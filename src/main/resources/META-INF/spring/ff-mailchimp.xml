<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="org.jahia.modules.ffmailchimp.actions.SaveConfiguration">
        <property name="publicationService" ref="jcrPublicationService" />
    </bean>
    <bean class="org.jahia.modules.ffmailchimp.actions.RemoveConfiguration">
        <property name="publicationService" ref="jcrPublicationService" />
    </bean>
    <bean class="org.jahia.modules.ffmailchimp.actions.RetrieveLists">
        <property name="schedulerService" ref="SchedulerService" />
    </bean>
    <bean class="org.jahia.modules.ffmailchimp.actions.RetrieveListMergeFields"/>
    <bean class="org.jahia.modules.ffmailchimp.actions.SubscribeToMailchimp"/>
    <bean class="org.jahia.modules.ffmailchimp.initializer.MailChimpInterestsInitializer"/>
</beans>